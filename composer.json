{
    "name": "rlacerda83/lumem-microservice-shipment",
    "description": "Microservice for management shipment.",
    "keywords": ["shipping", "laravel", "lumen", "microservice", "shipment", "correios", "ups"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "jeremeamia/superclosure": "~2.0",
        "dingo/api": "@dev",
        "rlacerda83/query-parser": "@dev",
        "rlacerda83/lumem-elocache": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
